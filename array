#include <stdio.h>

int main()
{
    int const column = 3;
    int const row = 2;
    
    int grade[row][column] = {
        {12, 23, 45},
        {64, 78, 89}};

        for (int i = 0; i < row; i++)
        {
            for (int k = 0; k < column; k++)
            {
             printf("%d ", grade[i][k]);
            }
            printf("\n");
        }


    return 0;
}
